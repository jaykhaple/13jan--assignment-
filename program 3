#include <iostream>
using namespace std;
class Shape {
    public:
     
        void area(int length, int breadth) {
            cout << "Area of rectangle: " << length * breadth << endl;
        }
       
        void area(int side) {
            cout << "Area of square: " << side * side << endl;
        }
};

int main() {
    Shape shape;
    shape.area(5,6); 
    shape.area(7); 
    return 0;
}
